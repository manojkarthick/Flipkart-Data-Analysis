            try{
                //HACK to fire omniture event for deferred modules. Why setTimeout? otherwise it would fire as load event.
                //Also sets value for all input fields with ugcData_tracker field name, which are passed while clicking Add_to_Cart or Buy_Now
                $("#omniClickEventSimulator").click(function(){
                    window.setTimeout(function(){
                        FKART.omni.customEvent("PPReviewsAvailable", {eVar45: "11_27_4.037"});
