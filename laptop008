        var ab = {"eVar34":"SELLER_TABLE_ENABLED","eVar32":"product_ads_on_productPage_enabled","eVar31":"disabled","eVar69":"PINCODE_WIDGET_POSITION_TOP","eVar39":"MULTISELECTION_SHOW","eVar51":"verification_flow"};

        for(var v in ab) {
            if(s && s.hasOwnProperty(v) && ab[v]) {
                s[v] = ab[v];
            }
        }
</script>
